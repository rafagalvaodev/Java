package desafio;

import java.security.InvalidParameterException;

public class Contadores {

	public String contar(int parametroOne, int parametroTwo) throws InvalidParameterException {

		int contagem = parametroOne - parametroTwo;
		if(parametroOne > parametroTwo) {
			throw new InvalidParameterException("O segundo parametro deve ser Maior do o primeiro");
		}else {

			for(int i = 0; i < contagem; i++) {
				System.out.println("Imprimendo o nÃºmero: " + (i+1));
			}
		
		}
		return null;
	}
}
